{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "poc-spire-adf"
		},
		"AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://poc-spire-keyvault.vault.azure.net/"
		},
		"adbLS_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0814-075749-7o019bqj"
		},
		"bimrailsdbLS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "poc-spire-dev.database.windows.net"
		},
		"bimrailsdbLS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "bimrailsdb"
		},
		"bimrailsdbLS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin_dev"
		},
		"pocspirelakeStorage_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "pocspirelake-conn1"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "poc-spire-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"adbLS_properties_typeProperties_existingClusterId": {
						"value": "[parameters('adbLS_properties_typeProperties_existingClusterId')]"
					},
					"bimrailsdbLS_properties_typeProperties_server": {
						"value": "[parameters('bimrailsdbLS_properties_typeProperties_server')]"
					},
					"bimrailsdbLS_properties_typeProperties_database": {
						"value": "[parameters('bimrailsdbLS_properties_typeProperties_database')]"
					},
					"bimrailsdbLS_properties_typeProperties_userName": {
						"value": "[parameters('bimrailsdbLS_properties_typeProperties_userName')]"
					},
					"pocspirelakeStorage_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('pocspirelakeStorage_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}